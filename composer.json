{
    "name": "prowebce/database-anonymizer",
    "type": "library",
    "description": "Database anonymizer.",
    "keywords": [
        "php",
        "database",
        "anonymizer"
    ],
    "homepage": "https://webnet.fr",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "doctrine/dbal": "^2.9.0|^3.0",
        "fakerphp/faker": "^1.14",
        "symfony/console": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/event-dispatcher": "^5.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^4.2",
        "phpunit/phpunit": "^9.3",
        "symfony/config": "^5.0",
        "symfony/yaml": "^5.0"
    },
    "suggest": {
        "symfony/config": "To configure anonymizer.",
        "symfony/yaml": "To configure anonymizer using yaml files."
    },
    "autoload": {
        "psr-4": {
            "WebnetFr\\DatabaseAnonymizer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebnetFr\\DatabaseAnonymizer\\Tests\\": "tests/DatabaseAnonymizer/Tests"
        }
    },
    "bin": ["bin/database-anonymizer"],
    "config": {
        "sort-packages": true
    }
}
